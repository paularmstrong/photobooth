{
  "name": "gopro-photobooth",
  "private": true,
  "engines": {
    "node": ">=v16.13"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "yarn workspaces foreach -p run build",
    "compile": "cross-env MODE=production yarn build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "test": "yarn workspaces foreach -p run test",
    "test:e2e": "yarn build && vitest run --passWithNoTests",
    "start": "node scripts/watch.js",
    "lint": "eslint . --cache --fix --ext js,cjs,ts,tsx",
    "tsc": "yarn workspaces foreach -p run tsc"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "^5.14.0",
    "cross-env": "7.0.3",
    "electron": "17.1.0",
    "electron-builder": "22.14.13",
    "electron-devtools-installer": "3.2.0",
    "eslint": "8.9.0",
    "happy-dom": "2.41.0",
    "nano-staged": "0.6.0",
    "playwright": "1.19.1",
    "prettier": "^2.5.1",
    "simple-git-hooks": "2.7.0",
    "typescript": "^4.5.5",
    "vitest": "^0.5.4"
  },
  "workspaces": [
    "packages/*"
  ]
}
